{
  "name": "dlpwait",
  "version": "0.0.0",
  "description": "A wait times app for Disneyland Paris",
  "scripts": {
    "prestart": "rimraf ./build",
    "start": "webpack-dev-server --open --config webpack.config.js --content-base ./public/dist",
    "prebuild": "rimraf ./build",
    "build": "cross-env NODE_ENV=production webpack --mode production --config webpack.config.js --optimize-minimize",
    "prewebpack": "rimraf ./build",
    "webpack": "webpack --watch --mode development --config webpack.config.js",
    "lint": "eslint -c ./package.json ./"
  },
  "engines": {
    "node": "^14.0.0"
  },
  "author": "Glenn de Haan",
  "license": "MIT",
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "rules": {
      "no-console": 0,
      "react/prop-types": 0,
      "comma-dangle": [
        "error",
        "never"
      ],
      "indent": [
        "error",
        4
      ]
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "settings": {
      "react": {
        "pragma": "h",
        "version": "16.0"
      }
    }
  },
  "eslintIgnore": [
    "public/dist",
    "tailwind.config.js",
    "postcss.config.js",
    "webpack.config.js"
  ],
  "dependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-transform-react-jsx": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "autoprefixer": "^10.4.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clsx": "^1.1.1",
    "copy-webpack-plugin": "^8.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.2",
    "css-minimizer-webpack-plugin": "^3.3.1",
    "eslint": "^7.22.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.17.0",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.3.9",
    "node-sass": "^5.0.0",
    "postcss-loader": "^6.2.1",
    "preact": "^10.1.1",
    "preact-router": "^4.0.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "rimraf": "^3.0.0",
    "sass-loader": "^11.0.1",
    "tailwindcss": "^2.2.19",
    "unistore": "^3.5.1",
    "uuid": "^8.3.2",
    "webpack": "^5.25.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  }
}
